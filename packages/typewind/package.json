{
  "name": "typewind",
  "version": "0.0.9",
  "description": "The safety of Typescript with the magic of Tailwind.",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "homepage": "https://typewind.vercel.app",
  "scripts": {
    "build": "tsup",
    "watch": "npm run build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mokshit06/typewind.git",
    "directory": "packages/typewind"
  },
  "bin": {
    "typewind": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./dist/*": "./dist/*",
    "./babel": {
      "types": "./dist/babel.d.ts",
      "default": "./dist/babel.js"
    },
    "./transform": {
      "types": "./dist/transform.d.ts",
      "default": "./dist/transform.js"
    }
  },
  "keywords": [
    "typescript",
    "tailwind",
    "tailwindcss",
    "babel-plugin",
    "zero-runtime"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "eval": "^0.1.8",
    "prettier": "^2.8.3",
    "tsup": "^6.5.0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.0",
    "@types/prettier": "^2.7.2"
  }
}